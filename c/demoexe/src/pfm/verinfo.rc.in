#include "winres.h"

VS_VERSION_INFO VERSIONINFO
   FILEVERSION @pvermjr@,@pvermin@,@pverpch@,@pvertwk@
   PRODUCTVERSION @PVERMJR@,@PVERMIN@,@PVERPCH@,@PVERTWK@
   FILEFLAGSMASK 0x3fL
#ifdef _DEBUG
   FILEFLAGS 0x1L
#else
   FILEFLAGS 0x0L
#endif
   FILEOS 0x40004L
   FILETYPE 0x1L
   FILESUBTYPE 0x0L
BEGIN
   BLOCK "StringFileInfo"
   BEGIN
      BLOCK "080404b0"
      BEGIN
         VALUE "CompanyName", "zhengrr"
         VALUE "FileDescription", "@pnom@"
         VALUE "FileVersion", "@pver@"
         VALUE "InternalName", "@pnom@"
         VALUE "LegalCopyright", "The MIT License"
         VALUE "OriginalFilename", "@pnom@.exe"
         VALUE "ProductName", "@PNOM@"
         VALUE "ProductVersion", "@PVER@"
      END
   END
   BLOCK "VarFileInfo"
   BEGIN
      VALUE "Translation", 0x804, 1200
   END
END
